    "model = FM(input_shape=train_X.shape[1], f=16, all_categories=all_categories)\n",
    "\n",
    "optimizer = torch.optim.Adam(model.parameters(), lr=lr, weight_decay=1e-5)\n",
    "criterion = nn.BCELoss()"
   ]
  },
  {
